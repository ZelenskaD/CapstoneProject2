{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "User Update",
    "type": "object",
    "properties": {
    "firstName": {
        "type": "string",
            "minLength": 1
    },
    "lastName": {
        "type": "string",
            "minLength": 1
    },
    "email": {
        "type": "string",
            "format": "email"
    },
    "deliveryAddress": {
        "type": "string",
            "minLength": 1
    },
    "preferences": {
        "type": "object",
            "additionalProperties": true
    },
    "password": {
        "type": "string",
            "minLength": 6
    },
    "isAdmin": {
        "type": "boolean"
    }
},
    "required": ["email"],
    "additionalProperties": false
}
