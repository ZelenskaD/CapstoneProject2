{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "User Update",
    "type": "object",
    "properties": {
    "firstName": {
        "type": "string",
            "minLength": 1
    },
    "lastName": {
        "type": "string",
            "minLength": 1
    },
    "email": {
        "type": "string",
            "format": "email"
    },
    "deliveryAddress": {
        "type": "string",
            "minLength": 1
    },

    "password": {
        "type": "string",
            "minLength": 6
    }
},
    "required": ["email"],
    "additionalProperties": false
}
