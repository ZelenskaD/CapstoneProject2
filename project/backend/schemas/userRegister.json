{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "User Register",
    "type": "object",
    "properties": {
    "username": {
        "type": "string",
            "minLength": 1
    },
    "password": {
        "type": "string",
            "minLength": 6
    },
        "firstName": {
            "type": "string",
            "minLength": 1
        },
        "lastName": {
            "type": "string",
            "minLength": 1
        },
    "email": {
        "type": "string",
            "format": "email"
    },

    "deliveryAddress": {
        "type": "string",
            "minLength": 1
    }
},
    "required": ["username", "password",  "firstName", "lastName","email", "deliveryAddress"],
    "additionalProperties": false
}
